{
  "name": "steffenbrand/non-static-php-jwt",
  "description": "non-static wrapper for php-jwt",
  "homepage": "https://github.com/steffenbrand/non-static-php-jwt",
  "type": "library",
  "authors": [
    {
      "name": "Steffen Brand",
      "email": "s.brand@steffenbrand.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1",
    "psr/container": "^1.0",
    "firebase/php-jwt": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "SteffenBrand\\NonStaticPhpJwt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SteffenBrand\\NonStaticPhpJwt\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}